  "exampleCodeblockParameters": "go title:foo",
  "exampleCodeblockContent": "package wp\n\nimport (\n\t\"fmt\"\n\t\"sync\"\n)\n\ntype Worker struct {\n\tid            int\n\tjobsCompleted int\n\tmu            *sync.Mutex\n}\n\nvar workers = []*Worker{\n\t{id: 1},\n\t{id: 2},\n\t{id: 3},\n\t{id: 4},\n\t{id: 5},\n\t{id: 6},\n\t{id: 7},\n\t{id: 8},\n\t{id: 9},\n\t{id: 10},\n}\n\n// Pool - worker pool\ntype Pool[Data any] struct {\n\tpool    chan *Worker\n\thandler func(int, Data)\n}\n\nfunc New[Data any](handler func(int, Data)) *Pool[Data] {\n\treturn &Pool[Data]{\n\t\tpool:    make(chan *Worker, len(workers)),\n\t\thandler: handler,\n\t}\n}\n\n// Create - puts all workers into the pool\nfunc (p *Pool[Data]) Create() {\n\tfor _, worker := range workers {\n\t\tp.pool <- worker\n\t}\n}\n\n// Handle - takes a worker from the pool and processes the data\nfunc (p *Pool[Data]) Handle(data Data) {\n\tworker := <-p.pool\n\n\tgo func() {\n\t\tp.handler(worker.id, data)\n\n\t\t// Mutex is not required as data is read from a channel\n\t\t// and processed by a single goroutine\n\t\tworker.jobsCompleted++\n\t\tp.pool <- worker\n\t}()\n}\n\n// Wait - waits for all workers to finish their jobs\nfunc (p *Pool[Data]) Wait() {\n\tfor range len(workers) {\n\t\t<-p.pool\n\t}\n}\n\nfunc (p *Pool[Data]) Stats() {\n\tfmt.Println(\"_____ Worker Stats _____\")\n\tfor _, worker := range workers {\n\t\tfmt.Printf(\"Worker %d completed %d jobs\\n\", worker.id, worker.jobsCompleted)\n\t}\n\tfmt.Println(\"________________________\")\n}",
